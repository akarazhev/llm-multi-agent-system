AGENT_CONFIG_PATH=config.yaml

CURSOR_WORKSPACE=/path/to/your/workspace

LOG_LEVEL=INFO
LOG_FILE=logs/agent_system.log

OUTPUT_DIRECTORY=./output

# LLM API Configuration
# The system uses AUTO model selection - just set your API key(s)
# It will automatically choose the best available model

# Anthropic Claude (Recommended)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenAI (Alternative)
OPENAI_API_KEY=your_openai_api_key_here

# Ollama (Local, Free)
OLLAMA_HOST=http://localhost:11434

# Optional: Override model selection
# ANTHROPIC_API_MODEL=claude-3-5-sonnet-latest
# OPENAI_API_MODEL=gpt-4o
